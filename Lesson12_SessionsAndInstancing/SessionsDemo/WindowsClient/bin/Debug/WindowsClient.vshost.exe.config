          <endpoint address="net.tcp://localhost:9000/OrderService"
                    binding="netTcpBinding"
                    bindingConfiguration="tcpBinding"
                    name="OrderService_Tcp"
                    contract="OrderServiceLibrary.IOrderService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
